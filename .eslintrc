{
  // "re"
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    // "es6": true,
    "es6": true,
    "browser": true,
    // webpack находится на платформе node.js
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "indent": "off"
    // "comma-dangle": "off"
  }
}
